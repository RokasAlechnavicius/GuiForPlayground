<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.KidQueueController"
            prefHeight="400.0" prefWidth="600.0">
    <headerText>
        Fill in the info
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Playground name: " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <ComboBox fx:id="playgroundComboBox" GridPane.rowIndex="0" GridPane.columnIndex="1"/>

            <Label text="Kid: " GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <ComboBox fx:id="kidComboBox" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
